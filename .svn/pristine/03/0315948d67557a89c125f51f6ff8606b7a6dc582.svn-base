        }
        
    }
    if (this.action === 'modificarSolicitud')
    {
        Traspasotpv.gridSearchVars('gridTraspasoproducto', '&idalmacendestino=' + Traspasotpv.get('idalmacendestino') + '&idalmacenorigen=' + $('#' + Traspasotpv.Id("idalmacenorigen")).val());
